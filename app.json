{
  "expo": {
    "name": "Hiryo2",
    "slug": "Hiryo2",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "net.suurisaitekikun.Hiryo",
      "buildNumber": "1.0.1"
    },
    "android": {
      "package": "net.suurisaitekikun.Hiryo",
      "versionCode": 2,
      "permissions": [],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive.png",
        "backgroundImage": "./assets/background.png",
        "permissions": [
          "ACCESS_NETWORK_STATE",
          "INTERNET"
        ]
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "79f5cd3b-76fd-417c-a3aa-fb73caed035a"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 31,
            "targetSdkVersion": 31,
            "buildToolsVersion": "31.0.0"
          },
          "ios": {
            "deploymentTarget": "13.0",
            "useFrameworks": "static"
          }
        }
      ]
    ]
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-4189981999969719~3191852158",
    "ios_app_id": "ca-app-pub-4189981999969719~4467568511",
    "delay_app_measurement_init": true,
    "user_tracking_usage_description": "許可することで、より関連性の高い広告を表示することができるようになります。"
  }
}
